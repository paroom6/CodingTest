package codingTest.ct.week01;

public class Ex0111 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 구구단
		 * 
		 */
		for(int i = 1; i < 10; i++) {
			for(int j = 2; j < 10; j++) {
				System.out.printf("%d * %d = %-2d \n", j, i, (i * j));
			}
			System.out.println();
		}
	}

}
