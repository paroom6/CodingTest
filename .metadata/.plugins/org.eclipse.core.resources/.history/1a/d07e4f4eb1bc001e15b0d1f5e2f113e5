package codingTest.ct.week01;

public class Ex0103 {

	public static void main(String[] args) {
		int i = 10; int j = 20;
		int k = -8;
		
		int a = i + j;
		int b = i - j;
		int c = i * j;
		int d = i / j;
		int e = i % j;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		
		
	}

}
