package codingTest.ct.week01;

public class Ex0105 {

	public static void main(String[] args) {
		int i = 3;
		switch (i) {
		case 1: 
			System.out.println("1분기");
		case 2: 
			System.out.println("2분기");
		case 3: 
			System.out.println("3분기");
		case 4: 
			System.out.println("4분기");
		default:
			System.out.println("알 수 없음");
		}
	}

}
